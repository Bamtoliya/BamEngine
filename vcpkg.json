{
  "name": "bam-engine",
  "version-string": "0.1.0",
  "dependencies": [
    "fmt",
    {
      "name": "sdl3",
      "features": ["vulkan"]
    },
    "glm",
    "vulkan",
    "glaze",
    "assimp",
    "imguizmo",
    "stb",
    "glslang",
    "spirv-tools",
    "spirv-cross",
    {
      "name": "imgui",
      "features": [
        "dx12-binding",
        "sdl3-binding",
        "vulkan-binding",
        "docking-experimental",
        "sdl3-renderer-binding",
        "sdlgpu3-binding"
      ]
    }
  ]
}